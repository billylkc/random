alias shtml strip_html $*
alias sl strip_linenum $*
alias gr go run main.go $*
alias rr go run cmd/main.go $*
alias gc git add . && git commit -m $*
alias gt command utility tag $*
alias gs git status
alias gd git diff --numstat $*
alias pp python main.py $*
alias gg go run main.go $*
alias ll ls -lash $*
alias ga git add . && git commit -m "w/e" $*
alias gitl git log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s" $*
alias gpp cd /Git/GoExample/src/github.com/billylkc $*
alias gp cd /Git $*
alias c clear  $*
alias cc clear 1
alias cs cht.sh $*
alias t todo $*
alias news command news $*
alias wiki command utility wiki $*
alias cmd command $*
alias ch command utility cheatsheet $*
