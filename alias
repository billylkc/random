alias chp command utility cheatsheet python $*
alias chr command utility cheatsheet r $*
alias clr find . -name '*.R' | xargs wc -l $*
alias clp find . -name '*.py' | xargs wc -l $*
alias clg find . -name '*.go' | xargs wc -l $*
